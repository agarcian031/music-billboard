<h1>Artist: </h1>
<%# <p>Find me in app/views/billboards/show.html.erb</p> %>
<div class="ui card">
  <div class="content">
    <p class="header">
      <%= @artist.name %>
    </p>
  </div>
  <div class="extra content">
    <%= link_to 'Edit', edit_artist_path(@artist), class: "ui button blue" %> |
    <%= link_to 'Show songs', artist_songs_path(@artist), class: "ui button blue" %> |
    <%= link_to 'Back', billboards_path, class: "ui button"%>
    <%= link_to 'Delete', artist_path(@artist), method: :delete, class: "ui button" %> 
  </div>
</div>